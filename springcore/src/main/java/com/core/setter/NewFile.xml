<?xml version="1.0" encoding="UTF-8"?>
<beans
   xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
    
    <bean name="student" class="com.core.setter.Student">
       <property name="id" value="1"/>
       <property name="name" value="emp1"/>
       <property name="address" value="texas"/>
       <property name="phone">
          <list>
            <value>111</value>
            <value>222</value>
            <value>333</value>
          </list>
       </property>
    </bean>
    <bean name="subject" class="com.core.setter.Subject">
       <property name="sub1" value="math"/>
       <property name="sub2" value="science"/>
       <property name="sub3" value="hsitory"/>
    </bean>               
</beans>