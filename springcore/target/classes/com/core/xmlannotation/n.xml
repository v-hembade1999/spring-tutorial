<?xml version="1.0" encoding="UTF-8"?>
<beans
   xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
    <bean name="emp" class="com.core.xmlannotation.Emp" autowire="byType">
      <property name="eid" value="1"/>
      <property name="ename" value="rock"/>
    </bean>
    <bean name="address" class="com.core.xmlannotation.Adress">
      <property name="empadress" value="pune"/>
      <property name="empcitycode" value="222"/>
    </bean>
</beans>